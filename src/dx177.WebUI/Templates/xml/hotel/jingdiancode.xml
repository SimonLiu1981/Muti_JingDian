  <HashDataMods>
    <HashDataMod>
      <Sql> select   '|景区名字|旅游攻略_交通_门票_酒店订房' as 网站标题,'游酷网：旅游攻略_交通_门票_酒店订信息最全的网站，和|景区名字|景区多加酒店、宾馆、餐饮合作，为您提供最好、性价比最高的服务！' as 网站内容,'|景区名字|旅游攻略,旅游交通,景点门票，旅游路线,旅游饮食,旅游酒店' as 网站关键字   </Sql>
      <HashKey>首页关键字</HashKey>
    </HashDataMod>
        
    <HashDataMod>
      <Sql>
        <![CDATA[ select    (select top 1  matchdomain from JingQus as jq  where  jq.jingqucode=h.JingQuCode)  AS JQCOD_PATH , h.* ,( select top 1 AreaName from JingQu_Area where JinqQuCode=h.area) as areaname ,m.showidx  
,  h.name  as hotelname  from Hotel h,Module m where h.guid = m.pguid and h.status=1 and modulecode='recommend' and h.JingQuCode='|JQCODE|' order by m.showidx desc  ]]>
      </Sql>
      <HashKey>推荐酒店</HashKey>
    </HashDataMod>
  
    <HashDataMod>
      <Sql>
        <![CDATA[ select top 6  h.* ,  (select top 1  matchdomain from JingQus as jq  where  jq.jingqucode=h.JingQuCode)  AS JQCOD_PATH , ( select top 1 AreaName from JingQu_Area ja where ja.JinqQuCode=h.JingQuCode and ja.JingQuAreaId = h.Area ) as areaname   from Hotel h  where  h.status=1 and h.minprice <100  and h.JingQuCode='|JQCODE|' and guid not in  (select pguid from Module )  order by h.minprice asc  ]]>
      </Sql>
      <HashKey>经济型酒店</HashKey>
    </HashDataMod>
   
    <HashDataMod>
      <Sql>
        <![CDATA[ select top 6  h.*  , (select top 1  matchdomain from JingQus as jq  where  jq.jingqucode=h.JingQuCode)  AS JQCOD_PATH , ( select top 1 AreaName from JingQu_Area ja where ja.JinqQuCode=h.JingQuCode and ja.JingQuAreaId = h.Area ) as areaname from Hotel h  where  h.status=1 and h.minprice >90  and  h.maxprice>120  and h.JingQuCode='|JQCODE|'  order by h.minprice desc  
      ]]>
      </Sql>
      <HashKey>星级酒店</HashKey>
    </HashDataMod>
 
    

   
    <HashDataMod>
      <Sql>
        <![CDATA[ select top 8 * ,   (select top 1  matchdomain from JingQus as jq  where  jq.jingqucode=Sites.JingQuCode)   AS JQCOD_PATH    from Sites  where jingqucode='|JQCODE|'  order by seqno desc  ]]>
      </Sql>
      <HashKey>景点欣赏</HashKey>
    </HashDataMod>


    <HashDataMod>
      <Sql>
        <![CDATA[ select top 10  *  ,convert(char(10),CREATE_DATE,20) as create_date1, (  select code from  Qsttype as qt where qt.guid =qtype  ) as QTYOECODE, (  select title from  Qsttype as qt where qt.guid =qtype  ) as SORTNAME ,   (select top 1  matchdomain from JingQus as jq  where  jq.jingqucode=Questions.JingQuCode)  AS JQCOD_PATH   from Questions   where jingqucode='|JQCODE|'  order by seqno desc  ]]>
      </Sql>
      <HashKey>景区问答</HashKey>
    </HashDataMod>
    



    <HashDataMod>
      <Sql>
        <![CDATA[ select top  10  * ,  (select top 1  matchdomain from JingQus as jq  where  jq.jingqucode=news.JingQuCode)  AS JQCOD_PATH  from  news  where TGUID in ( select guid from newstype where code='LYGL')  and  jingqucode='|JQCODE|' 
   ]]>
      </Sql>
      <HashKey>旅游攻略</HashKey>
    </HashDataMod>

    <HashDataMod>
      <Sql>
        <![CDATA[   select  top 4 * ,  (select top 1  matchdomain from JingQus as jq  where  jq.jingqucode=a.JingQuCode) AS JQCOD_PATH  from  Questions   as a  where    jingqucode='|JQCODE|'   order by seqno desc  
   ]]>
      </Sql>
      <HashKey>疑难问答</HashKey>
    </HashDataMod>

    <HashDataMod>
      <Sql>
        <![CDATA[select top 6 *  ,  (select top 1  matchdomain from JingQus as jq  where  jq.jingqucode=News.JingQuCode) AS JQCOD_PATH  from News  where  jingqucode='|JQCODE|'  order by seqno desc 
   ]]>
