  <HashDataMods>
    <HashDataMod>
      <Sql>select   '景点旅游攻略_交通_酒店_门票预订网：游酷网' as 网站标题,'景点旅游攻略/交通/门票/旅游订房信息最全的网站，和全国各地酒店、宾馆、餐饮合作，为您提供最好、性价比最高的服务！' as 网站内容,'旅游攻略，交通，门票,酒店预订' as 网站关键字  </Sql>
      <HashKey>首页关键字</HashKey>
    </HashDataMod>

    <HashDataMod>
      <Sql>
        <![CDATA[ select    (select top 1  matchdomain from JingQus as jq  where  jq.jingqucode=h.JingQuCode)  AS JQCOD_PATH , h.* ,( select top 1 AreaName from JingQu_Area where JinqQuCode=h.area) as areaname ,m.showidx  
,  h.name  as hotelname  from Hotel h,Module m where h.guid = m.pguid and h.status=1 and modulecode='recommend' and h.JingQuCode='dxs' order by m.showidx desc  ]]>
      </Sql>
      <HashKey>丹霞山推荐酒店</HashKey>
    </HashDataMod>
    <HashDataMod>
      <Sql>
        <![CDATA[ select    (select top 1  matchdomain from JingQus as jq  where  jq.jingqucode=h.JingQuCode)  AS JQCOD_PATH , h.* ,( select top 1 AreaName from JingQu_Area where JinqQuCode=h.area) as areaname ,m.showidx  
,  h.name  as hotelname  from Hotel h,Module m where h.guid = m.pguid and h.status=1 and modulecode='recommend' and h.JingQuCode='zjj' order by m.showidx desc  
      ]]>
      </Sql>
      <HashKey>张家界推荐酒店</HashKey>
    </HashDataMod>

    <HashDataMod>
      <Sql>
        <![CDATA[ select top 6  h.* ,  (select top 1  matchdomain from JingQus as jq  where  jq.jingqucode=h.JingQuCode)  AS JQCOD_PATH , ( select top 1 AreaName from JingQu_Area ja where ja.JinqQuCode=h.JingQuCode and ja.JingQuAreaId = h.Area ) as areaname   from Hotel h  where  h.status=1 and h.minprice <100  and h.JingQuCode='dxs' and guid not in  (select pguid from Module )  order by h.minprice asc  ]]>
      </Sql>
      <HashKey>丹霞山经济型酒店</HashKey>
    </HashDataMod>
    <HashDataMod>
      <Sql>
        <![CDATA[select top 6  h.* ,  (select top 1  matchdomain from JingQus as jq  where  jq.jingqucode=h.JingQuCode) AS JQCOD_PATH , ( select top 1 AreaName from JingQu_Area ja where ja.JinqQuCode=h.JingQuCode and ja.JingQuAreaId = h.Area ) as areaname  from Hotel h  where  h.status=1 and h.minprice <100  and h.JingQuCode='zjj'  and guid not in  (select pguid from Module )  order by h.minprice asc  ]]>
      </Sql>
      <HashKey>张家界经济型酒店</HashKey>
    </HashDataMod>
    <HashDataMod>
      <Sql>
        <![CDATA[ select top 6  h.*  , (select top 1  matchdomain from JingQus as jq  where  jq.jingqucode=h.JingQuCode)  AS JQCOD_PATH , ( select top 1 AreaName from JingQu_Area ja where ja.JinqQuCode=h.JingQuCode and ja.JingQuAreaId = h.Area ) as areaname from Hotel h  where  h.status=1 and h.minprice >90  and  h.maxprice>120  and h.JingQuCode='dxs'  order by h.minprice desc  
