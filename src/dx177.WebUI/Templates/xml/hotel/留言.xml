                                        function LeaveMessageSubmit() {
                                            if ($('#mess_username').val() == '') {
                                                alert('请输入称呢！');
                                                $('#mess_username').focus();
                                                return false;
                                            }
                                            if ($('#mess_email').val() == '') {
                                                alert('请输入邮箱地址！');
                                                $('#mess_email').focus();
                                                return false;
                                            }
                                            else {
                                                if (!isEmail($('#mess_email').val(), "邮箱格式不正确!")) {
                                                    $('#mess_email').focus();
                                                    return false;
                                                }
                                            }
                                            if ($('#mess_tel').val() == '') {
                                                alert('请输入联系电话！');
                                                $('#mess_tel').focus();
                                                return false;
                                            }
                                            if ($('#mess_message').val() == '') {
                                                alert('请输入内容！');
                                                $('#mess_message').focus();
                                                return false;
                                            }
                                            $.getJSON("/ajaxHandler.aspx?MethodName=SubmitLeaveMessage&amp;jsoncallback=?",
									            { mess_username: encodeURIComponent($('#mess_username').val()),
									                mess_tel: encodeURIComponent($('#mess_tel').val()),
									                mess_email: encodeURIComponent($('#mess_email').val()),
									                mess_message: encodeURIComponent($('#mess_message').val())
									            },
									            function(json) {
									                alert("提交成功！");
									                window.location.href = window.location.href;
									            });
                                        }
                                    &lt;/script&gt;

