    <HashDataMod>
      <Sql>SELECT *  ,  (select top 1  matchdomain from JingQus as jq  where  jq.jingqucode=s.JingQuCode)   as  JQRP_CODE FROM Sites s where s.JingQuCode='|JINGQUCODE|'  order by ViewTimes desc </Sql>
